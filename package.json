{
  "name": "pully",
  "version": "1.0.0",
  "description": "Realtime messaging infrastructure",
  "main": "lib/index.js",
  "author": "David Ajayi",
  "license": "ISC",
  "scripts": {
    "cdk:bootstrap": "cdk bootstrap --context @aws-cdk/core:newStyleStackSynthesis=1 --cloudformation-execution-policies arn:aws:iam::aws:policy/AdministratorAccess",
    "cdk:synth": "mkdir ./lib/cdk-out && npm run cdk:bootstrap && cdk synth",
    "cdk:deploy": "npm run cdk:bootstrap && cdk deploy '*' --outputs-file cdk_outputs.json",
    "cdk:destroy": "cdk destroy"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.101.0"
  }
}
